{
  "name": "ft-widget",
  "version": "1.0.0",
  "description": "A kitchen-sink demo of the FileThis ft-widget components",
  "main": "ft-widget.js",
  "module": "ft-widget.js",
  "type": "module",
  "scripts": {
    "lint": "lit-analyzer ft-widget.js && eslint '**/*.js'",
    "format": "prettier ft-widget.js test/* --write",
    "docs": "npm run docs:clean && npm run analyze && npm run docs:build && npm run docs:assets && npm run docs:gen",
    "docs:clean": "rimraf docs",
    "docs:gen": "eleventy --config=.eleventy.cjs",
    "docs:gen:watch": "eleventy --config=.eleventy.cjs --watch",
    "docs:build": "rollup -c --file docs/ft-widget.bundled.js",
    "docs:assets": "cp node_modules/prismjs/themes/prism-okaidia.css docs/",
    "docs:serve": "es-dev-server --root-dir=docs --node-resolve --watch",
    "analyze": "wca analyze \"**/*.js\" --outFile custom-elements.json",
    "serve": "es-dev-server --node-resolve --watch",
    "test": "karma start karma.conf.cjs",
    "test:watch": "karma start karma.conf.cjs --auto-watch=true --single-run=false",
    "test:update-snapshots": "karma start karma.conf.cjs --update-snapshots",
    "test:prune-snapshots": "karma start karma.conf.cjs --prune-snapshots",
    "checksize": "rollup -c ; cat ft-widget.bundled.js | gzip -9 | wc -c ; rm ft-widget.bundled.js",
    "build": "echo \"This is not a TypeScript project, so no need to build.\"",
    "filethis:open": "open http://127.0.0.1:8000/dev",
    "filethis:serve": "es-dev-server --node-resolve --watch --open http://127.0.0.1:8000/dev",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "javascript"
  ],
  "author": "The Polymer Authors",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "@filethis/mx-design-tokens": "^1.0.1",
    "@material/mwc-button": "^0.21.0",
    "@webcomponents/template": "^1.4.2",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "core-js": "^3.6.5",
    "lit": "^2.0.0-rc.1",
    "lit-flexbox-literals": "0.0.7",
    "lit-html": "^1.2.1",
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@babel/core": "^7.14.0",
    "@open-wc/testing": "^2.5.10",
    "@open-wc/testing-karma": "^3.3.11",
    "@rollup/plugin-replace": "^2.3.1",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/web-components": "^6.2.9",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@web/rollup-plugin-copy": "^0.2.0",
    "@web/rollup-plugin-html": "^1.6.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "deepmerge": "^4.2.2",
    "es-dev-server": "^1.46.1",
    "eslint": "^6.8.0",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "lit-analyzer": "^1.1.9",
    "mocha": "^7.1.1",
    "prettier": "^2.0.4",
    "rollup": "^2.47.0",
    "rollup-plugin-summary": "^1.3.0",
    "web-component-analyzer": "^1.0.3"
  }
}
